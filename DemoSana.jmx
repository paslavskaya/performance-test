def count = Integer.parseInt(vars.get(&quot;plpProductId_matchNr&quot;))
	for (def i = 0; i &lt; count; i++) {
		def plpProductId = vars.get(&quot;plpProductId_&quot; + (i+1) )
			plpProductIds &lt;&lt; plpProductId
		def plpAuthToken = vars.get(&quot;plpAuthToken_&quot; + (i+1) )
			plpAuthTokens &lt;&lt; plpAuthToken
	}

int idx = new Random().nextInt(plpProductIds.size());

String randomPLPProductId = (plpProductIds[idx]);
String randomPLPAuthToken = (plpAuthTokens[idx]);

vars.put(&quot;randomPLPProductId&quot;, randomPLPProductId);
vars.put(&quot;randomPLPAuthToken&quot;, randomPLPAuthToken);</stringProp>
