		def searchProductId = vars.get(&quot;searchProductId_&quot; + (i+1) )
			searchProductIds &lt;&lt; searchProductId
		def searchAuthToken = vars.get(&quot;searchAuthToken_&quot; + (i+1) )
			searchAuthTokens &lt;&lt; searchAuthToken
	}

int idx = new Random().nextInt(searchProductIds.size());

String randomSearchProductId = (searchProductIds[idx]);
String randomSearchAuthToken = (searchAuthTokens[idx]);

